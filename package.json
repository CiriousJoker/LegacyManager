{
  "name": "legacy-manager",
  "productName": "Legacy Manager",
  "description": "The game launcher and updater for Super Smash Bros. Legacy",
  "version": "0.1.3",
  "author": "CiriousJoker <ciriousjoker@gmail.com>",
  "copyright": "Â© 2017 CiriousJoker",
  "homepage": "http://www.smashbroslegacy.com/",
  "license": "MIT",
  "main": "app/background.js",
  "build": {
    "appId": "com.manager.legacy",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraFiles": [
      "src/cs/**/*",
      ".server"
    ],
    "publish": {
      "provider": "github",
      "owner": "CiriousJoker"
    }
  },
  "scripts": {
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build",
    "start": "gulp start",
    "pretest": "gulp build-unit --env=test",
    "test": "electron-mocha app/specs.js.autogenerated --renderer --require source-map-support/register",
    "coverage": "npm test -- -R scripts/istanbul-reporter",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register"
  },
  "dependencies": {
    "7zip-bin": "^2.1.0",
    "child_process": "^1.0.2",
    "edge-asar-cs": "^1.0.3",
    "edge-cs": "^1.2.1",
    "electron-devtools-installer": "^2.2.0",
    "electron-edge": "^6.5.5",
    "electron-settings": "^3.1.1",
    "electron-updater": "^2.8.6",
    "file-exists": "^4.0.0",
    "fs-extra": "^3.0.1",
    "fs-jetpack": "^1.0.0",
    "immutability-helper": "^2.3.0",
    "is-valid-path": "^0.1.1",
    "line-by-line": "^0.1.5",
    "material-ui": "1.0.0-beta.3",
    "material-ui-icons": "^1.0.0-alpha.19",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "react": "^15.6.1",
    "react-dom": "^15.4.2",
    "react-fade-in": "^0.1.4",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "request": "^2.81.0",
    "slash": "^1.0.0",
    "synchronize": "^2.0.0",
    "walkdir": "0.0.11"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babili-webpack-plugin": "^0.0.11",
    "chai": "^4.1.0",
    "electron": ">=9.4.0"
    "electron-builder": "^19.16.0",
    "electron-mocha": "^4.0.0",
    "electron-packager": "^8.7.0",
    "electron-rebuild": "^1.6.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-less": "^3.3.2",
    "gulp-plumber": "^1.1.0",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^0.4.5",
    "minimist": "^1.2.0",
    "mocha": "^3.4.2",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-url": "^1.2.0",
    "source-map-support": "^0.4.15",
    "spectron": "^3.7.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": ">=3.1.11"
  }
}
